{
    "name": "posthtml-favicons",
    "version": "1.3.1",
    "description": "PostHTML Favicon generator ",
    "main": "dist/src/index.js",
    "types": "dist/src/index.d.ts",
    "repository": "git@github.com:mohsen1/posthtml-favicons.git",
    "author": "Mohsen Azimi <me@azimi.me>",
    "license": "MIT",
    "scripts": {
        "test": "jest",
        "preintegration": "yarn build",
        "integration": "ts-node integration.ts",
        "prepublish": "yarn build",
        "build": "tsc --pretty",
        "prebuild": "rm -rf dist",
        "format": "prettier --write '**/*.{json,md,ts,tsx,yml}'"
    },
    "devDependencies": {
        "@types/jest": "^26.0.10",
        "@types/mkdirp": "^1.0.1",
        "@types/node": "^14.6.0",
        "@types/rimraf": "^3.0.0",
        "jest": "^26.4.2",
        "posthtml": "^0.13.3",
        "prettier": "^2.1.0",
        "ts-jest": "^26.2.0",
        "ts-node": "^9.0.0",
        "typescript": "^3.9.7"
    },
    "dependencies": {
        "favicons": "^6.2.0",
        "mkdirp": "^1.0.4",
        "posthtml-parser": "^0.5.0",
        "tslib": "^2.0.1"
    },
    "jest": {
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ],
        "modulePathIgnorePatterns": [
            "dist/__tests__"
        ]
    }
}
