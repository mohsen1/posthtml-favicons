{
    "name": "posthtml-favicons",
    "version": "1.3.1",
    "description": "PostHTML Favicon generator ",
    "main": "dist/src/index.js",
    "types": "dist/src/index.d.ts",
    "repository": "git@github.com:mohsen1/posthtml-favicons.git",
    "author": "Mohsen Azimi <me@azimi.me>",
    "license": "MIT",
    "scripts": {
        "test": "jest",
        "preintegration": "yarn build",
        "integration": "ts-node integration.ts",
        "prepublish": "yarn build",
        "build": "tsc --pretty",
        "prebuild": "rm -rf dist"
    },
    "devDependencies": {
        "@types/jest": "^24.0.23",
        "@types/node": "^12.12.17",
        "jest": "^24.9.0",
        "posthtml": "^0.12.0",
        "ts-jest": "^24.2.0",
        "ts-node": "^8.5.4",
        "typescript": "^3.7.3"
    },
    "dependencies": {
        "@types/mkdirp": "^0.5.2",
        "@types/rimraf": "^2.0.3",
        "favicons": "^6.2.0",
        "mkdirp": "^0.5.1",
        "posthtml-parser": "^0.4.2",
        "tslib": "^1.10.0"
    },
    "jest": {
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ],
        "modulePathIgnorePatterns": [
            "dist/__tests__"
        ]
    }
}
